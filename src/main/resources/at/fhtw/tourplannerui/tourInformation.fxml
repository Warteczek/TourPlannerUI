<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<TabPane prefHeight="350.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="General information">
            <content>
                <HBox prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <VBox prefHeight="297.0" prefWidth="323.0">
                            <children>
                                <Label text="Name:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="infoName" />
                                <Label text="From: ">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="infoFrom" />
                                <Label text="To:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="infoTo" />
                                <Label text="Transport type:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font></Label>
                                <Label fx:id="infoTransType" />
                                <Label text="Distance:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>

                                <Label fx:id="infoDistance" />
                                <Label text="Time:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="infoTime" />
                            </children>
                        </VBox>
                        <VBox prefHeight="297.0" prefWidth="336.0">
                            <children>
                                <Label text="Description:">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="infoDescription" />
                            </children>
                        </VBox>
                        <VBox prefHeight="297.0" prefWidth="272.0">
                            <children>
                                <Label text="Popularity">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="popularity" />
                                <Label text="Child friendly?">
                                    <font>
                                        <Font name="System Bold" size="12.0" />
                                    </font>
                                </Label>
                                <Label fx:id="childFriendliness" />
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </content>
        </Tab>
        <Tab text="Route">
            <content>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <ImageView fx:id="routeImage" fitHeight="299.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
                        <Label fx:id="routeErrorLabel" />
                    </children>
                </HBox>
            </content>
        </Tab>
    </tabs>
</TabPane>